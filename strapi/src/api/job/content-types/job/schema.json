{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "job"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 150
    },
    "Description": {
      "type": "richtext",
      "required": true,
      "minLength": 10
    },
    "Screenshots": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "link": {
      "type": "string"
    },
    "stacks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stack.stack",
      "mappedBy": "stack"
    }
  }
}
